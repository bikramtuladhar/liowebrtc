{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "extends": "airbnb",
  "rules": {
    "max-len": 0,
    "no-multi-assign": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "consistent-return": 0,
    "guard-for-in": 0
    }
}
